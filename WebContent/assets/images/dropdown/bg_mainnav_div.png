    }
        
}
function escapeKeyword(keyword) {
        var escapedKeyword = "";
        for (var i=0; i<keyword.length; i++) {
            var cc = keyword.charCodeAt(i); 
            if ((cc>=65 && cc<=90) || (cc>=97 && cc<=122)) {
                escapedKeyword += keyword.charAt(i);
            } else {
                if (cc==47) {
                    escapedKeyword += "";
                } else {
                    var cc16 = cc.toString(16);
                    if (cc16.length==2) {
                        escapedKeyword += "%"+cc16;
                    } else {
                        escapedKeyword += "";
                    }
                }
            }
        }
        return escapedKeyword;
    }
