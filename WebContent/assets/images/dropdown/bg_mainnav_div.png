<script type="text/javascript">
    var player = $("div#videoplayer embed");
	if(!player) {
		player = $("div#player embed");
	}
    var screenWidth = document.body.clientWidth;
    if (player) {
			
		var width = $(player).attr("width");
		var height = $(player).attr("height");          
		if (width>(screenWidth-40)) {
			var ratio = height/width;
			width = screenWidth-40;
			height = ratio*width;
		}
		$(player).attr("width", width);
		$(player).attr("height", height);

	}  
    var noflashimg = $("div#videoplayer a img"); 
	if(!noflashimg) {
		noflashimg = $("div#player a img");
	}
    if (noflashimg){
        var imgSrc = $(noflashimg).attr("src");        
        if (imgSrc.indexOf("install-flash-graphic")>-1){
            var width = 640;
            var height = 360;          
            if (width>(screenWidth-40)) {
                var ratio = height/width;
                width = screenWidth-40;
                height = ratio*width;
            }
            $(noflashimg).attr("width", width);
            $(noflashimg).attr("height", height);
        }
    }
</script></div>
