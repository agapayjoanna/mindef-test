function stripXSS(s, isNameField) {
        var str = "";
        var len = s.length;
        for (var i = 0; i < len; i++) {
            var ch = s.charAt(i);
            switch (ch) {
                case '<': {
                  break;
                }
                case '>': {            
                  break;
                }
                case '(': {
                  break;
                }
                case ')': {
                  break;
                }
                case '#': {
                  break;
                }
                case '&': {       
                  break;
                }
                case '"': {       
                  break;
                }
                case '\'': {      
                  break;
                }
                case '{': {       
                  break;
                }
                case '}': {       
                  break;
                }
                case ';': {       
                  if(isNameField) {
                     break;
                  }
                }
                case ':': {       
                  if(isNameField) {
                     break;
                  }
                }
                case ',': {       
                  if(isNameField) {
                     break;
                  }
                }
                default: {
                      str = str.concat(ch);
                }
            }
        } 
        return str;
    }
    
